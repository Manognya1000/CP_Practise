    "n = int(input())\n",
    "\n",
    "A = list(map(int,input().split()))\n",
    "A.sort()\n",
    "\n",
    "M = 1000005\n",
    "c = [0]*M\n",
    "for i in A:\n",
    "    if c[i] != 0:\n",
    "        c[i] = 2\n",
    "        continue\n",
    "    \n",
    "    for j in range(i,M,i):\n",
    "        c[j] += 1\n",
    "\n",
    "ans = 0\n",
    "for i in A:\n",
    "    if c[i] == 1:\n",
    "        ans += 1\n",
    "        \n",
    "print(ans)"
